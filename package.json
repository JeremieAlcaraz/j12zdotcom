{
  "name": "site-formations",
  "version": "1.0.0",
  "description": "Site vitrine moderne pour prÃ©senter formations et services",
  "type": "module",
  "author": "Votre Nom",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "sync": "astro sync",
    "clean": "rm -rf dist .astro",
    "build:prod": "pnpm run clean && pnpm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.svelte",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.svelte --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "img:avif": "mkdir -p src/assets/img_opt && pnpm exec sharp -i 'src/assets/raw/*.{png,jpg,jpeg}' -o 'src/assets/img_opt/{name}.avif' -f avif -q 75 resize 1500",
    "img:webp": "mkdir -p src/assets/img_opt && pnpm exec sharp -i 'src/assets/raw/*.{png,jpg,jpeg}' -o 'src/assets/img_opt/{name}.webp' -f webp -q 75 resize 1500",
    "img:png": "mkdir -p src/assets/img_opt && pnpm exec sharp -i 'src/assets/raw/*.{png,jpg,jpeg}' -o 'src/assets/img_opt/{name}.png'  -f png  -q 80 resize 1500",
    "img:opt": "pnpm run img:avif && pnpm run img:webp && pnpm run img:png",
    "build": "pnpm run img:opt && astro check && astro build",
    "preview": "astro preview",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/svelte": "^7.1.0",
    "astro": "^5.11.0",
    "daisyui": "^5.0.43",
    "github-slugger": "^2.0.0",
    "lucide-react": "^0.525.0",
    "marked": "^16.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "svelte": "^5.35.2",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-svelte": "^3.10.1",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sass": "^1.89.2",
    "sharp-cli": "^5.2.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.4.0",
  "keywords": [
    "astro",
    "tailwind-css",
    "tailwindcss-v4",
    "daisyui",
    "react",
    "svelte",
    "formations",
    "site-vitrine"
  ]
}
