# .env.example - Variables d'environnement pour le projet

# Cloudflare Tunnel Token
# Obtenir depuis: https://dash.cloudflare.com/ > Zero Trust > Access > Tunnels
CLOUDFLARE_TUNNEL_TOKEN=your_tunnel_token_here

# Email pour Let's Encrypt (certificats SSL Caddy)
LETSENCRYPT_EMAIL=contact@jeremiealcaraz.com

# Environnement (development, staging, production)
NODE_ENV=production

# Port pour le serveur statique en production
STATIC_SERVER_PORT=8080

# Port Astro dev
ASTRO_DEV_PORT=4321
